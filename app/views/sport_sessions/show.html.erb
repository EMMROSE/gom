<div class="container mt-5">
  <h1>Show Session</h1>
  <li> Session Members:
      <ul>
    <% @sport_session.session_users.each do |session_user| %>
        <% if session_user.owner %>
          <li><b><%= session_user.user.username %> <%= "(owner)"  %></b></li>
        <% else %>
          <li><%= session_user.user.username %></li>
        <% end %>
    <% end %>
      </ul>
  </li>
  <li><%= @sport_session.title %></li>
  <li><%=  @sport_session.description %></li>
  <li><%= @sport_session.start_time %></li>
  <li><%= @sport_session.level %></li>
  <li><a href="#" class="btn btn-gom">Join this Session!</a></li>
</div>
